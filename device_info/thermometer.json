{
  "device_driver": "default",
  "generation_interval": 1,
  "coil": {
    "start_addr": 0,
    "end_addr": 9999
  },
  "discrete_input": {
    "start_addr": 10000,
    "end_addr": 19999
  },
  "input_register": {
    "start_addr": 30000,
    "end_addr": 39999,
    "data": [
      {
        "name": "temperature",
        "addr": 30000,
        "default": [0,0,0],
        "interval": 5,
        "min": [0, -50,0],
        "max": [1, 50,999],
        "error_rate": 10
      },
      {
        "name": "humidity",
        "addr": 30005,
        "default": [0,0],
        "interval": 3,
        "min": [0,0],
        "max": [99,99],
        "error_rate": 10
      }
    ]
  },
  "holding_register": {
    "start_addr": 40000,
    "end_addr": 49999
  }
}